	  $('#searchbtn').bind("click",function(){
	      var txt=$('#searchtxt').val();
		  var re=/[\!\@\#\$\%\^\&\*\(\)\……\——\（\）\~\·\`\<\>\?\\\,\.\;\'\"\《\，\。\；\‘\:\：\“\？\》\、\￥\[\]\{\}\【\】]+/;
		  
		  if(re.test(txt)){
				$("<div>").html("含有非法字符").appendTo("body").dialog();
